{
  "name": "resto-apps",
  "version": "1.0.0",
  "description": "miniproject code.id academy",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "test": "nodemon app.js",
    "dbcreate":"sequelize db:create & sequelize db:migrate",
    "dbdrop":"sequelize db:drop",
    "dbgen":"sequelize init & npm run model_resto & npm run model_food & npm run model_ingredient & npm run model_food-ingredient",
    "model_resto":"sequelize model:generate --name restaurant --attributes name:string",
    "model_food":"sequelize model:generate --name food --attributes name:string,price:integer,restaurantId:integer",
    "model_ingredient":"sequelize model:generate --name ingredient --attributes name:string",
    "model_food-ingredient":"sequelize model:generate --name FIjunction --attributes foodId:integer,ingredientId:integer"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arelhabib/resto-apps.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/arelhabib/resto-apps/issues"
  },
  "homepage": "https://github.com/arelhabib/resto-apps#readme",
  "dependencies": {
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "pg": "^8.10.0",
    "sequelize": "^6.30.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "sequelize-cli": "^6.6.0"
  }
}
